            {
              "name": "Identify text marked up as a heading",
              "id": "headingsYes",
              "resource_path": "checks/headings",
              "css": "headings.css",
              "addScript": "headings.js",
              "removeScript": "headings-remove.js"
            },
            {
              "name": "Verify large bold text is not a heading",
